{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "singleQuote": true,
  "semi": true,
  "printWidth": 100,
  "tabWidth": 2,
  "trailingComma": "es5",
  "arrowParens": "always",
  "importOrder": [
    "^react$",
    "^(react|react-dom)(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^.+\\.(css|scss)$",
    "^[./]"
  ],
  "importOrderSeparation": true
}
